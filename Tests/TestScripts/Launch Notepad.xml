<?xml version="1.0"?>
<Script>
  <Name>Launch Notepad</Name>
  <Description>Launches notepad and waits for exit.</Description>
  <Category>Maintenance</Category>
  <SafetyLevel>Safe</SafetyLevel>
  <Impact>Ergonomics</Impact>
  <Code>
    <Execute Host="Cmd">%windir%\System32\notepad.exe</Execute>
  </Code>
</Script>